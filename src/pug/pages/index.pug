        span.s3d-filter__amount-flat__num.js-s3d__amount-flat__num-all 456

    .s3d-popup-flyby__wrap.js-s3d-popup-flyby
      .s3d-popup-flyby__active.js-s3d-popup-flyby__active
      .s3d-popup-flyby__bg
        .s3d-popup-flyby__bg-active.js-s3d-popup-flyby__bg-active
        .s3d-popup-flyby__bg-all
      .s3d-popup-flyby
        .s3d-popup-flyby__close(data-type="close")
